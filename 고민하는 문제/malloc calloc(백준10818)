#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int min_(int arr[], int n){
    int i;
    int min = arr[0];
    
    for(i=1; i<n; i++)
        if(min>arr[i])  min = arr[i];
        
    return min;
}

int max_(int arr[], int n){
    int i;
    int max = arr[0];
    
    for(i=1; i<n; i++)
        if(max<arr[i])  max = arr[i];
    
    return max;
}

int main(){
    int *arr;
    int n, i;
    arr = (int*)malloc(sizeof(int));        // malloc 쓰면 틀림, calloc쓰면 맞음 왜그럴까..
    
    
    scanf("%d", &n);
    
    for(i=0; i<n; i++)
        scanf("%d", &arr[i]);
    
    printf("%d %d", min_(arr,n), max_(arr,n));  
    free(arr);
}
